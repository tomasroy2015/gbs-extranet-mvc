    function btnSave_Click() {
        var conditions = [];

        $.each(propertyConditions, function (key, val) {
            var condition = {};
            var isChecked = $("#checkbox" + val.AttributeId.toString() + ':checked').length > 0;
            if (isChecked) {
                condition.AttributeID = $("#checkbox" + val.AttributeId.toString() + ':checked').val();
                //var test = document.getElementById('drp' + val.AttributeHeaderCode + 'unit');
                //if ($(this).find('drp' + val.AttributeHeaderCode + 'unit') !== 'undefined') {
                //    UnitID.push(document.getElementById('drp' + val.AttributeHeaderCode + 'unit').value == null ? -1 : document.getElementById('drp' + val.AttributeHeaderCode + 'unit').value);
                //}
                //if ($(this).find('txt' + val.AttributeHeaderCode) !== 'undefined') {
                //    Price.push(document.getElementById('txt' + val.AttributeHeaderCode).value == null ? 0 : document.getElementById('txt' + val.AttributeHeaderCode).value);
                //}
                //if ($(this).find('drp' + val.AttributeHeaderCode + 'currency') !== 'undefined') {
                //    CurrencyID.push(document.getElementById('drp' + val.AttributeHeaderCode + 'currency').value == null ? -1 : document.getElementById('drp' + val.AttributeHeaderCode + 'currency').value);
                //}

                if (val.Chargeable) {
                    var untId = document.getElementById('drp' + val.AttributeId + 'unit').value;
                    condition.UnitID = untId;
                    var currId = document.getElementById('drp' + val.AttributeId + 'currency').value;
                    condition.CurrencyID = currId;
                    condition.Price = document.getElementById('txt' + val.AttributeId).value;
                    condition.Charged = 1;
                    //condition.Unitvalue = document.getElementById('drp' + val.AttributeHeaderCode + 'currency');
                } else {
                    condition.UnitID = 0;
                    condition.CurrencyID = '@ViewBag.HotelcurrencyID';
                    condition.Price = 0.00;
                    condition.Charged = 0;
                    // condition.Unitvalue = "0.00";
                }
                conditions.push(condition);
            }
        });

        $("#imgloader").show();
        $.ajax({
            url: "/PropertyCondition/SavePropertyConditions",
            type: "POST",
            data: JSON.stringify({ Conditions: conditions }),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (res) {
                if (res != 0) {
                    $("#imgloader").hide();

                    document.getElementById("lblSuccess").style.display = "";
                    setTimeout("Hidelabel()", 10000);
                }
            }
        });
    }


