}

<h2>AdminHotelReservation</h2>
<script>
    $(document).ready(function () 
    {
        usershowreservation();
    });
    function usershowreservation() {
        var ReservationID = '@ViewBag.ReservationID'
        var json = { ReservationID: ReservationID }


        if (ReservationID != "") {
            $.ajax({
                type: "get",
                url: "/AdminHotelReservation/GetReservation",
                data: json,
                dataType: "Json",
                traditional: true,
                success: function (res) {
                }
            })
        }
    }
        </script>



